
public class TesteSacaNegativo {
	public static void main(String[] args) {
		Conta conta = new Conta(); //Conta é um nome de uma classe que existe no sistema e "conta" uma variável
		conta.deposita(100);
		System.out.println(conta.saca(200));
		System.out.println(conta.saldo);
		
		conta.saldo = conta.saldo - 101;//no atributo saldo eu digo que ele vale o quanto valia antes -101
		System.out.println(conta.saldo);
	}

}
